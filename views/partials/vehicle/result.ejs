<div class="col-lg-4 col-md-4 col-sm-6">
    <div class="car">

        <% if (vehicle.image_urls.length > 0 && vehicle.image_urls[0].length > 0) { %>
            <img src="<%=UtilityService.crop_url(vehicle.image_urls[0], 330, 440, 'crop')  %>" alt="Used car deals on a <%= vehicle.year %> <%= vehicle.make %> <%= vehicle.model %>">
        <% } else { %>
            <img src="/images/default-photo.png" alt="Used car deals on a <%= vehicle.year %> <%= vehicle.make %> <%= vehicle.model %>">
        <% }  %>

        <div class="overlay">
            <h3>
                <a href="/<%= vehicle.zip_code %>/<%= vehicle.vehicle_id %>">
                    <%= vehicle.year %> <%= vehicle.make %> <%= vehicle.model %>
                </a>
            </h3>
            <strong>Price: $<%= vehicle.price %></strong></div>
    </div>
    <div class="other-info clearfix"><span>Mileage: <%= vehicle.mileage %></span><small>Location: <%= vehicle.city %>, <%= vehicle.state %> </small></div>
</div>