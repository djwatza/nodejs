            <form action="/inventory/search" method="GET" id="main-search">
                <div class="row" id="primary-search">
                    <div class="col-lg-6 col-md-6 clearfix">
                        <div class="form-group clearfix">
                            <label for="autocomplete" class="control-label">Enter your zip code to find used cars for sale near you:</label>
                        </div>
                    </div>

                    <div class="col-lg-6 col-md-6 clearfix">
                        <div class="form-group clearfix">
                            <input type="text" data-ng-model="suggestions.input" typeahead-min-length="3" placeholder="Zip code" typeahead="city for city in suggestions.suggest($viewValue)" class="form-control">
                        </div>
                    </div>
                </div>

                <div class="row hidden" id="secondary-search">
                    <div class="col-lg-6 col-md-6 clearfix">
                        <div class="form-group clearfix">
                            <div class="col-lg-8 col-md-8 col-sm-6">
                                <select class="form-control make" name="makes" id="makes">
                                    <option>Filter By Make</option>
                                </select>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-6">
                                <select class="form-control year" name="year" id="year" disabled>
                                    <option>Select Year</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-8 col-md-8 col-sm-6">
                                <select class="form-control model" name="model" id="model" disabled>
                                    <option>(Select Make)</option>
                                </select>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-6">

                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6">
                        <div class="sort">
                            <label class="control-label">Sort by</label>
                        </div>
                        <div class="buttons"> <a href="#">Price</a> <a href="#">Mileage</a> <a href="#">Distance</a> </div>
                    </div>
                </div>
            </form>
        </div>
        </div>
    </section>